EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/BXCodeScanner" "$CONFIGURATION_BUILD_DIR/PinAutoLayout"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/BXCodeScanner/BXCodeScanner.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/PinAutoLayout/PinAutoLayout.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "BXCodeScanner" -framework "PinAutoLayout"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_ROOT = ${SRCROOT}/Pods
